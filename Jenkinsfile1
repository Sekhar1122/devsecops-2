node ('Appserver-agent'){  
    def app
    stage('Cloning Git') {
        sh 'echo Clone the code'
    }  
    
        stage('Build and Tag') {
        sh 'echo Clone the code'
    } 
        stage('Cloning Git') {
        sh 'echo Clone the code'
    } 
    
        stage('Push to Dockerhub') {
        sh 'echo Clone the code'
    } 
    
        stage('pull from dockerhub') {
        sh 'echo Clone the code'
    } 

}
